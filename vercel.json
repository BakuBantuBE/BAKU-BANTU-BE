{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ],
    "outputDirectory": "build",
    "crons": [
      {
        "path": "/api/cron/inject-wilayah",
        "schedule": "0 9 * * *"
      },
      {
        "path": "/api/cron/cleanup-wilayah",
        "schedule": "0 21 * * *"
      }
    ]
  }